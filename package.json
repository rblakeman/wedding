        "lint": "eslint . && prettier . --check",
        "format": "eslint . --fix && prettier . --write",
        "typecheck": "tsc",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
